{
  "name": "spryker/validator-extension",
  "type": "library",
  "description": "ValidatorExtension module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/symfony": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use the ValidatorPluginInterface.",
    "spryker/symfony": "If you want to use the ValidatorPluginInterface."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
